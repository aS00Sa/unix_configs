[alias]
    st = status
    co = checkout
    ci = commit
    bbranch = branch -v
    lg = log --graph --pretty=format:'%C(yellow)%h%Creset -%C(green)%d%Creset %s %C(red)(%cr) %C(yellow)<%an>%Creset' --abbrev-commit --date=relative
    llg = log --pretty=format:'%C(yellow)%h %Cred%ad %C(yellow)%an%C(green)%d %Creset%s' --date=iso
[user]
    email = olshevskiy87@bk.ru
    name = Dmitriy Olshevskiy
[push]
    default = current
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
